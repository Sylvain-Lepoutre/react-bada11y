{
  "name": "@inolib/react-bada11y",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "format": "pnpm run format:prettier && pnpm run format:jsdoc && pnpm run format:jsxattribute-sort && pnpm run format:packagejson && pnpm run format:sort-imports && pnpm run format:tailwindcss",
    "format:jsdoc": "prettier --config ./.config/prettier/jsdoc/prettier.config.js --write ./**/*.cjs ./**/*.js ./**/*.ts ./**/*.tsx",
    "format:jsxattribute-sort": "prettier --config ./.config/prettier/jsxattribute-sort/prettier.config.js --write ./**/*.tsx",
    "format:packagejson": "prettier --config ./.config/prettier/packagejson/prettier.config.js --write ./package.json",
    "format:prettier": "prettier --write .",
    "format:sort-imports": "prettier --config ./.config/prettier/sort-imports/prettier.config.js --write ./**/*.js ./**/*.ts ./**/*.tsx",
    "format:tailwindcss": "prettier --config ./.config/prettier/tailwindcss/prettier.config.js --write ./**/*.tsx",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint --ext .cjs,.js,.ts,.tsx .",
    "lint:tsc": "tsc",
    "serve:dev": "vite",
    "serve:preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "happy-dom": "^10.11.2",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.1",
    "prettier-plugin-jsxattribute-sort": "^1.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  }
}
