{
  "name": "@inolib/react-bada11y",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "format": "pnpm run format:prettier && pnpm run format:jsdoc && pnpm run format:jsxattribute-sort && pnpm run format:packagejson && pnpm run format:sort-imports && pnpm run format:tailwindcss",
    "format:jsdoc": "prettier --config ./.config/prettier/jsdoc/prettier.config.js --write ./**/*.cjs ./**/*.js ./**/*.ts ./**/*.tsx",
    "format:jsxattribute-sort": "prettier --config ./.config/prettier/jsxattribute-sort/prettier.config.js --write ./**/*.tsx",
    "format:packagejson": "prettier --config ./.config/prettier/packagejson/prettier.config.js --write ./package.json",
    "format:prettier": "prettier --write .",
    "format:sort-imports": "prettier --config ./.config/prettier/sort-imports/prettier.config.js --write ./**/*.js ./**/*.ts ./**/*.tsx",
    "format:tailwindcss": "prettier --config ./.config/prettier/tailwindcss/prettier.config.js --write ./**/*.tsx",
    "lint": "pnpm run lint:eslint && pnpm run lint:astro && pnpm run lint:tsc",
    "lint:astro": "astro check",
    "lint:eslint": "eslint --ext .astro,.cjs,.js,.ts,.tsx .",
    "lint:tsc": "tsc",
    "serve:dev": "astro dev",
    "serve:preview": "astro build && astro preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fontsource/atkinson-hyperlegible": "^5.0.17",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.4",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/prism": "^3.0.0",
    "@astrojs/react": "^3.0.4",
    "@astrojs/tailwind": "^5.0.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "astro": "^3.4.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "happy-dom": "^10.11.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "prettier-plugin-jsdoc": "^1.1.1",
    "prettier-plugin-jsxattribute-sort": "^1.0.1",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
